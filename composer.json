{
    "name": "senhormrtroll/azure-servicebus-laravel",
    "description": "Extending the Laravel queue to support the Microsoft Azure Service Bus Queues & topics.",
    "version": "8.0.0",
    "type": "library",
    "keywords": [
        "laravel",
        "azure",
        "servicebus",
        "queues"
    ],
    "license": "MIT",
    "authors": [{
        "name": "Senhor Mr Troll",
        "email": "senhormrtroll@gmail.com"
    }],
    "repositories": [{
        "type": "git",
        "url": "https://github.com/jonatansantana/azure-sdk-for-php.git"
    }],
    "require": {
        "php": ">=7.1.3",
        "illuminate/container": ">=8.0.0 <9.0.0",
        "illuminate/support": ">=8.0.0 <9.0.0",
        "illuminate/encryption": ">=8.0.0 <9.0.0",
        "illuminate/queue": ">=8.0.0 <9.0.0",
        "senhormrtroll/azureservicebuslaravel": "1.0.0"
    },
    "autoload": {
        "psr-4": {
            "SenhorMrTroll\\LaravelAzureServicebus\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "SenhorMrTroll\\LaravelAzureServicebus\\Support\\ServiceProvider"
            ]
        }
    }
}